{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "ESNext",
    "lib": ["ES2021", "DOM"],
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "allowJs": false,
    "esModuleInterop": false,
